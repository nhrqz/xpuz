{
  "name": "xpuz",
  "version": "1.1.0",
  "description": "Parses and creates crossword puzzle files",
  "main": "lib/index.js",
  "module": "src/index.js",
  "browser": "dist/xpuz.js",
  "scripts": {
    "test": "mocha-webpack --webpack-config ./webpack/test.config.js test",
    "test:watch": "mocha-webpack --watch --webpack-config ./webpack/test.config.js test",
    "prepublish": "npm run compile && npm run build",
    "build": "NODE_ENV=production webpack -p",
    "compile": "rm -rf lib; babel -s true -d lib src",
    "watch": "rm -rf lib; babel --watch --source-maps -s true -d lib src",
    "docs": "rm -rf docs; jsdoc -R README.md -p --package ./package.json -c ./.jsdoc.config.json -t ./node_modules/ink-docstrap/template -r -d docs",
    "docs:watch": "chokidar \"**/*.jsdoc\" \"src/**/*.js\" --verbose -i node_modules -i docs -i dist -c \"npm run docs\"",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/turnerhayes/xpuz.git"
  },
  "keywords": [
    "puz",
    "ipuz",
    "puzzle",
    "puzzles",
    "crossword"
  ],
  "author": "Turner Hayes <dev@turnerhayes.com>",
  "license": "GPL-3.0",
  "engines": {
    "node": "8.6.0",
    "npm": "5.4.2"
  },
  "bugs": {
    "url": "https://github.com/turnerhayes/xpuz/issues"
  },
  "homepage": "https://github.com/turnerhayes/xpuz#readme",
  "files": [
    "lib",
    "dist",
    "immutable.js"
  ],
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.3.3",
    "@types/buffer-reader": "^0.1.0",
    "@types/lodash": "^4.14.136",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-plugin-transform-undefined-to-void": "^6.9.4",
    "chai": "^4.1.2",
    "chokidar": "^1.7.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.1.0",
    "expect.js": "^0.3.1",
    "immutable": "^3.8.2",
    "ink-docstrap": "^1.3.2",
    "mocha": "^5.2.0",
    "mocha-webpack": "^1.1.0",
    "temp": "^0.8.3",
    "ts-loader": "^6.0.4",
    "typescript": "^3.5.3",
    "webpack": "^4.36.1",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "buffer-reader": "^0.1.0",
    "debug": "^3.1.0",
    "hash-it": "^2.1.2",
    "iconv-lite": "^0.4.24",
    "lodash": "^4.17.10"
  },
  "peerDependencies": {
    "immutable": ">=3"
  }
}
